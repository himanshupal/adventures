{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "lldb",
      "request": "launch",
      "name": "Debug",
      "program": "${workspaceFolder}/target/debug/rust",
      "cargo": {
        "args": ["build", "--bin=rust", "--package=rust"],
        "filter": {
          "name": "rust",
          "kind": "bin"
        }
      },
      "cwd": "${workspaceFolder}"
    }
  ]
}
